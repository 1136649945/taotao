.set_type p span{margin-right: 10px;}
.set_type p a{font-size: 20px;color:#494949;}

/*奖学金*/
.related_links{overflow: hidden;background-color: #f5f5f5;padding: 10px;}
.related_links li{float: left;width: 50%;text-align: center;padding: 10px 0;}
.related_links li a{font-size: 20px;color:#585858;}
.related_links li a:hover{color: #b51c26;}
.scholarship{}
.scholarship h2{font-size: 20px;color: #595959;margin: 20px 0;}
.scholarship p{font-size: 16px;color: #7c7c7c;margin: 5px 0;}
.links{overflow: hidden;}
.links li{float:left;width: 29.333%;margin: 3% 2%;text-align: center;}
.links li a.links_img{display: block;width: 100%;height: 100px;border:solid 1px #cccccc;}
.links li a.links_img img{display: inline-block;height: 60px;margin-top: 20px;}
/*.links li a.links_img:hover{border:solid 1px #b51c26;}*/
.links li p{margin-top: 40px;
    overflow: hidden;
    white-space: nowrap;
