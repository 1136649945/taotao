.links li:hover p,.links li:hover p a{color:#b51c26;}

/*新闻通告*/
.newsType{width:100%;margin-bottom: 15px;text-align: center;}
.newsType a{font-size: 16px;color:#696969;}
.newsType a.current{color: #b51c26;}
.newsType span{margin: 0 20px;font-size: 16px;}
.notice{width: 100%;margin: 30px 0 80px;}
.notice li{width:100%;height: 165px;overflow: hidden;margin:30px 0;background-color: #f8f8f8;}
.noticeL{float: left;overflow: hidden;width: 32%;}
.noticeL img{display: inline-block;width:100%;height: 100%;}
.noticeR{float: left;padding: 20px;width: 66%;}
.noticeR h3,.noticeR h3 a{font-size:16px;color: #6f6f6f;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;}
.noticeR h3:hover,.noticeR h3 a:hover{color: #b51c26;}
.noticeR p{font-size:14px;color: #9b9b9b;line-height: 26px;margin-top: 15px;}
.noticeR p a{font-size: 14px;color: #b51c26;}
.date{margin-top: 20px;font-size: 14px;color: #9b9b9b;text-align: right;}
.date span{margin-left: 20px;}
.date em{display: inline-block;width: 25px;height: 1px;background-color: #b51c26;margin-left: -30px;margin-bottom: -5px;}
.page{text-align: right;margin-bottom: 50px;}
/*.page input{font-size:14px;color:#4d5155;display: inline-block;width: 60px;height: 26px;line-height: 24px;text-align:center;border:solid 1px #ebebeb;background-color: transparent;}*/
.page a{font-size: 14px;color:#4d5155;display: inline-block;width: 26px;height: 26px;line-height: 24px;text-align:center;border:solid 1px #ebebeb;}
.page a.btnPage{width: 60px;}
/*.page input:hover,*/.page a:hover{color: #b51c26;}
.page a.current{background-color: #860000;border: solid 1px #860000;color: #fff;}
.meet{width: 100%;padding: 25px 15px;margin:10px 0;border:solid 1px #c9c9c9;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    behavior: url(../ie-css3.htc); /* 通知IE浏览器调用脚本作用于'box'类 */
}
.meet_imgs{width: 100%;overflow: hidden;}
.meet_imgs ul{width: 500%;}
.meet_imgs ul li{/*width: 20%;*/float: left;}
.meet_imgs ul li a,.meet_imgs ul li a img{display: block;width: 100%;}
.meet_imgs ul li p{width:100%;font-size: 20px;color: #494949;margin: 25px 0 35px;text-align: center;}
.meet_btn{text-align: center;}
.meet_btn span{cursor:pointer;display: inline-block;width: 10px;height: 10px;margin: 0 5px;background-color: #9a1923;
    -webkit-border-radius: 100%;
    -moz-border-radius: 100%;
    border-radius: 100%;
    behavior: url(../ie-css3.htc); /* 通知IE浏览器调用脚本作用于'box'类 */
}
.meet_btn span.current{background-color: #fc2e3e;}
/*学生组织*/
.meet_organ{padding: 5px;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
