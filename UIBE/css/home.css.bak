.navUl li.current img.ia2{display: inline-block;}
.navOl{display:none;margin-left:55px;padding: 15px 0;}
.navOl li{margin: 5px auto;}
.navOl li a{font-size: 14px;color: #666664;}
.navOl li a:hover{color: #b51c26;}

/*视频*/
.navUl_video li.current a.navUl_a{border-bottom: 0;}
/*友情链接*/
.navUl_link li.current a.navUl_a{border-bottom: 0;}
/*奖学金*/
.navUl_ship li a.navUl_a em{margin-left: 20px;}
.navUl_ship li .navOl{margin-left: 20px;}
/*新闻通告*/
.navUl_news{background-color: #f5f5f5;padding-bottom: 80px;}
.navUl_news li{padding: 0 10px;}
.navUl_news li a.navUl_a{overflow:hidden;height:inherit;line-height: inherit;vertical-align: middle;padding: 20px 0;}
.navUl_news li a.navUl_a em{display:block;margin: 0 15px;}
.navUl_news li.current a.navUl_a{color: #b51c26;border-left: 0;background-color: inherit;border-bottom: inherit;}
.navUl_news li.current a.navUl_a em,
.navUl_news li a.navUl_a em:hover{color: #b51c26;}
/*学生组织*/
.navUl_organ{padding-bottom: 40px;}
.navUl_organ li.current a.navUl_a{border-bottom: 0;}
/*留学贸大*/
.navUl_abroad{background: url("../images/icon/bg.jpg") no-repeat 100% 100%;padding-bottom: 140px;}

.dime{width: 100%;}
._dime{width: 100%;padding:8px;border:solid 1px #d2d2d2;}
.dime a{display: block;width: 100%;overflow: hidden;}
._dime a img{display: block;width: 100%;}
.dime p{text-align: center;font-size: 20px;color:#6c6c6c;margin-top: 5px;}
.contR{width: 100%;padding-left: 310px;}
.cont_box{width: 100%;border:solid 1px #d2d2d2;padding: 2px;overflow: hidden;}
.bannerR{width: 100%;overflow: hidden;}
.bannerR img{display: block;width: 100%;}
.headline{width: 100%;height: 42px;line-height: 42px;background-color: #f3eddd;}
.headline,.headline a{font-size: 14px;color:#565656;}
.headline span.hl_icon{display: inline-block;width: 18px;height: 16px;margin:0 10px -2px;background: url("../images/icon/home.png") no-repeat;}
.matterBox{padding: 20px;}
.set_box{width: 100%;overflow: hidden;}
.setup{width: 50%;padding: 30px;float: left;}
.setup li{/*float: left;*/width: 100%;/*margin: 0 2%;*//*padding: 45px 40px;*/padding:45px 0;border-bottom: dashed 1px #b5b5b5;}
.set_tit{width: 100%;height: 20px;line-height:20px;padding:0 8px;background-color: #f6f7fb;position: relative;}
.set_tit em{font-size: 24px;color: #6c6c6c;margin-top: -12px;float: left;}
.set_tit span{cursor:pointer;float: right;font-size: 20px;color: #494949;}
.set_tit img{position: absolute;top:0;right: 0;}
