body{position: relative;}
/*header*/
.header{width:100%;height: 50px;line-height: 50px;background-color: #eaeaea;color:#727272;z-index:11;position: fixed;top:0;left: 0;}
/*.header_left,.header_right{height:50px;}*/
.header_left{font-size: 18px;}
.header_left a{color:#727272;}
.header_left a:hover,
.header_left a.current{color:#8f000b;font-weight: bold}
.header_left em{display:inline-block;margin: 0 15px;margin-bottom:-2px;width: 1px;height:14px;background-color: #727272;}
._share{margin-top: 8px;margin-right: 20px;}
._share a{margin-right: 5px;}
.header_phone{display: none;}
.language{position: relative;}
.languageCur{width: 170px;height:24px;background-color: #fff;margin-top: 8px;position: relative;
    -webkit-border-radius: 12px;
    -moz-border-radius: 12px;
    border-radius: 12px;
    behavior: url(../ie-css3.htc); /* 通知IE浏览器调用脚本作用于'box'类 */
}
.languageBox{position: absolute;top:0;left: 0;height:24px;line-height: 24px;}
.languageBox span{cursor:pointer;display:inline-block;text-align:center;width: 90px;height:24px;color: #b5b5b5;font-size: 16px;line-height: 24px;

}
.languageBox span:first-child{width: 50px;}
.languageBox span.current{background-color: #8f000b;color: #fff;
    -webkit-border-radius: 12px;
    -moz-border-radius: 12px;
    border-radius: 12px;
    behavior: url(../ie-css3.htc); /* 通知IE浏览器调用脚本作用于'box'类 */
}
#push{position: absolute;right: 6px;top:8px;cursor: pointer;}
.languageMore{position: absolute;top:34px;left: 0;width: 100%;z-index: 2;height:0;overflow: hidden}
.languageMore li{cursor:pointer;width: 100%;height:30px;line-height: 30px;text-align: center;background-color: #fff;margin-bottom: 1px;}
.languageMore li:hover{background-color: #700005;color: #fff;}

/*logo-search*/
.logo-search{width:100%;padding: 27px 0;margin-top: 50px;}
.logoEn{padding-left: 20px;padding-top: 36px;}
.logoEn img{}
/*seacrh*/
.search{margin-top: 40px;margin-right: 15px;}
.key_box{width: 330px;height:40px;background-color: #f5f5f5;border:solid 1px #dbdbdb;padding: 7px 15px;}
.key_box input{letter-spacing:3px;font-size: 16px;color: #8e8e8e;border:0;background-color: transparent;width: 100%;}
._search{display:inline-block;width:124px;height:40px;background-color: #8f000b;text-align: center;}
._search img{margin-top: 8px;}
.problem{display:block;margin-top:45px;position: relative;font-size: 18px;color:#8e8e8e;}
.problem img{position: absolute;top:-32px;right:-10px;}
.problem.current img{}


/*menu*/
.menu{width: 100%;height:78px;background-color: #8f000b;}
.menu_ul{width: 105%;margin-left: -2.5%;}
.menu_ul li{float: left;width: 14.28571%;text-align:center;height:78px;line-height: 78px;position: relative;}
.menu_ul li a{display: block;width:100%;height:100%;color:#fff;font-size: 26px;}
.menu_ul li.current{background-color: #700005;}
.nav{position: absolute;z-index: 11;height: 0;overflow: hidden;width: 100%}
.menu_ul li:hover{background-color: #700005;}
.menu_ul li:hover .nav{height:auto;
    -webkit-transition: all 0.3s ease-in;
    -moz-transition: all 0.3s ease-in;
    -ms-transition: all 0.3s ease-in;
    -o-transition: all 0.3s ease-in;
    transition: all 0.3s ease-in;
}
.menu_ul li .nav p{padding: 10px 5px;margin-bottom: 1px;background-color: #8f000b;}
.menu_ul li .nav p:hover{background-color: #700005;}
.menu_ul li .nav p a{font-size: 15px;}

/*banner*/
.banner{
    -webkit-box-shadow: 0 6px 12px rgba(136, 136, 136, 0.175);
    box-shadow: 0 6px 12px rgba(136, 136, 136, 0.175);
}
.banner_bg{width: 100%;}
.banner_bg img{display:block;width: 100%;}

/*footer*/
.footer_addr{width: 100%;height:156px;background-color: #8f000b;color:#fff;text-align: center;padding-top: 40px;}
.footer_addr h4{font-size: 26px;margin-bottom: 20px;}
.footer_addr p{font-size: 20px;}
.footer_addr p em{font-size: 18px;margin-left: 30px;}
.footer_tel{width: 100%;height:78px;line-height:78px;background-color: #fff;color:#1d2637;font-size:20px;text-align: center;}
.footer_tel img{margin: 0 15px;}

/*item*/
.item{width: 100%;background-color: #fafafa;padding: 60px 0;}
.tit_l{font-size: 48px;padding: 12px 0;color:#0f2b5d;border-bottom: solid 4px #8f000b;}
.tit_more{font-size: 20px;color: #6b6b6b;margin-top: 70px;}
.tit_more:hover{color:#8f000b;}
.tit_br{font-size: 22px;color:#838383;position: relative;/*margin-top: 30px;*/margin-top:15px;}
.tit_br span{z-index:1;background-color:#fafafa;position: absolute;left: 0;top:-16px;}
.announcement .tit_br span{background-color:#fff;}
.tit_br em{position:absolute;left:0;display:block;width:100%;height:1px;background-color: #838383;}
.tit_br span.tit_bg{background-color: #f7f7f7;}
.item_box{overflow: hidden;width: 100%;margin-top: 50px;padding-bottom: 10px;}
.item_left,.item_right{width: 50%;}
.item_left{padding-right: 15px;}
.il_box{width: 100%;border:solid 1px #898989;}
.ir_list{width: 100%;}
.ir_list li{width: 100%;overflow: hidden;padding:35px 20px;background-color: #fafafa;}
.ir_list li:hover,
.ir_list li.current{background-color: #fff;}
.ir_date{width: 15%;}
.ir_date h5{width: 45px;height: 45px;text-align:center;line-height:46px;background-color: #e7e7e7;font-size:20px;color:#8c8b8b;}
.ir_date p{background-color:#fff;width: 45px;height: 45px;line-height:25px;text-align:center;border:solid 1px #fff;border-top:0;padding: 6px 3px;}
.ir_date p em{display: block;margin-bottom:6px;width: 7px;height:1px;background-color: #8c8b8b;}
.ir_date span{font-size: 24px;color: #a6a6a6;}
.ir_cont{width: 85%;}
.ir_cont h3{overflow: hidden;}
.ir_cont h3 a{font-size: 20px;display: inline-block;color: #727272;}
.ir_cont h3 .item_add{float:right;display: block;width: 18px;height:18px;margin-top: 7px;position: relative;}
.item_add img:last-child{position: absolute;top:0;left: 0;
    filter:alpha(opacity=0);
    -moz-opacity:0;
    -khtml-opacity: 0;
    opacity: 0;
}
.ir_cont p,.ir_cont p a{font-size: 16px;color: #b0b0b0;line-height: 24px;margin-top: 12px;text-align: justify;}
.ir_list li:hover .ir_cont h3 a{color: #8f000b;}
.ir_list li:hover .item_add img:last-child{
    filter:alpha(opacity=100);
    -moz-opacity:1;
    -khtml-opacity: 1;
    opacity: 1;
}
.ir_list li:hover .ir_date h5{background-color: #8f000b;color: #fff;}
.ir_list li:hover .ir_date p{border: solid 1px #d7d7d7;color:#8f000b;}
.ir_list li:hover .ir_date p em{background-color: #d50b23;}
.ir_list li:hover .ir_date span{color: #8f000b;}
.item_imgs{}
.item_imgs img{display: inline-block;width: 31%;margin-left: 1.5%;}
.il_box{position: relative;overflow: hidden;}
.il_auto{width: 100%;}
.il_list{width: auto;}
.il_list li{width: 100%;float: left;}
.il_list li .il_img,.il_list li .il_img img{display: block;width: 100%;}
.il_list li .ir_date h5{background-color: #8f000b;color: #fff;}
.il_list li .ir_date p{border: solid 1px #d7d7d7;color:#8f000b;}
.il_list li .ir_date p em{background-color: #d50b23;}
.il_list li .ir_date span{color: #8f000b;}
.il_cont{padding: 50px 15px;height: 250px;border: solid 1px #afafaf;border-top: 0;
    -webkit-box-shadow: 0 6px 12px rgba(175, 175, 175, 0.175);
    box-shadow: 0 6px 12px rgba(175, 175, 175, 0.175);
}
.il_list li .ir_date .il_p2{border: 0;background-color: transparent;overflow: hidden;}
.il_list li .ir_date .il_p2 span{display:inline-block;font-size: 16px;color:#a6a6a6;}
.il_list li .ir_date .il_p2 em{display:inline-block;float: left;}
._btn{position: absolute;right:15px;bottom: 15px;}
._btn span{display:inline-block;margin:0 20px;height:18px;width: 1px;background-color: #666666;}
._btn img{cursor: pointer;}
.il_list li{display: none;}
.il_list li.current{display: block;}



/*announcement*/
.announcement{width: 100%;background-color: #fff;padding: 60px 0;}
.anno_cont{overflow: hidden;width:100%;}
.anno_cont li{float:left;width: 33.333%;padding: 30px;}
.anno_cont li a,.anno_cont li a img{display: block;width:100%;overflow: hidden;}
.anno_tit{display:block;cursor:pointer;width:100%;height:82px;padding: 15px;background-color: #f6f8fa;text-align: center}
.anno_tit h3{font-size:14px;color:#aaaaaa;overflow: hidden;margin: 0 auto;}
.anno_tit h3 span{float: left;}
.anno_tit h3 em{float: right;display: inline-block;width: 34px;height:2px;margin-top: 10px;background-color: #aaaaaa;}
.anno_tit p{font-size: 18px;color:#00399f;margin-top: 6px;max-width: 100%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.anno_tit p span{display:inline-block;max-width: 100%;}
.anno_cont li:hover .anno_tit{background-color: #d50b23;}
.anno_cont li:hover .anno_tit p{color:#fff;}
.anno_cont li:hover .anno_tit,.anno_cont li:hover .anno_tit p{
    -webkit-transition: all 0.3s ease-in;
    -moz-transition: all 0.3s ease-in;
    -ms-transition: all 0.3s ease-in;
    -o-transition: all 0.3s ease-in;
    transition: all 0.3s ease-in;
}

/*news*/
.news{width: 100%;background-color: #f7f7f7;padding: 60px 0 0;}
.news_list,.news_list li{width: 100%;}
.news_list{margin-top: 30px;}
.news_list li{padding: 55px 0;border-bottom: solid 1px #cacaca;
    -webkit-box-shadow: 0 6px 12px rgba(136, 136, 136, 0.175);
    box-shadow: 0 6px 12px rgba(136, 136, 136, 0.175);
}
.news_list li:hover,
.news_list li.current{background-color: #fff;border-bottom: solid 1px #888;}
.n_date{width:6%;color:#0f2b5d;text-align: right;padding-right: 15px;}
.n_date h4{font-size: 40px;}
.n_date h5{font-size: 20px;}
.n_br{width:6%;text-align: center;}
.n_br em{display: inline-block;width: 24px;height:5px;background-color: #d50b23;margin-top: 12px;}
.n_img{width: 20%;overflow: hidden;}
.n_img img{width: 100%;display: inline-block;}
.n_cont{width: 68%;padding: 0 15px 0 30px;}
.n_cont h3,.n_cont h3 a{font-size: 24px;color:#252525;margin-bottom: 35px;}
.n_cont p,.n_cont h3{font-size: 18px;color:#929292;line-height: 30px;}
.n_cont h3 a:hover,.n_cont p a:hover{color:#d50b23;}
.n_cont p{text-align: justify;}

/*department*/
.department{width: 100%;height:682px;padding: 45px 0;margin-bottom:44px;background: url("../images/departmentbg.jpg") center no-repeat;}
.depar_tit{}
.depar_tit span{display: inline-block;font-size: 48px;color:#fff;padding: 20px 0;border-bottom: solid 4px #8f000b;}
.depar_box{margin-top: 80px;}
.depar_list{width: 80%;overflow: hidden;}
.depar_list li{cursor:pointer;float: left;width: 47%;margin-right:2%;height:96px;position: relative;}
.type{height:96px;line-height: 96px;}
.type img{display: inline-block;width: 48px;margin-bottom: -18px;}
.type span{font-size: 28px;color:#fff;margin-left: 60px;}
.type2{width: 100%;height:100%;position: absolute;left:0;top:0;background-color: #cacbce;padding: 10px 30px;
    filter:alpha(opacity=0);
    -moz-opacity:0;
    -khtml-opacity: 0;
    opacity: 0;
}
.type2 span{font-size: 28px;color:#262626;padding: 10px 0;border-bottom: solid 6px #d50b23;}
.type2 img{margin-top: 28px;}
.depar_list li:hover .type2,
.depar_list li.current .type2{
    filter:alpha(opacity=100);
    -moz-opacity:1;
    -khtml-opacity: 1;
    opacity: 1;
    -webkit-transition: all 0.3s ease-in;
    -moz-transition: all 0.3s ease-in;
    -ms-transition: all 0.3s ease-in;
    -o-transition: all 0.3s ease-in;
    transition: all 0.3s ease-in;
}
.depar_map{width: 20%;}
.dituContent{width: 100%;height: 300px;}
.share{width: 100%;margin-top: 15px;}
.share ul{overflow: hidden;}
.share ul li{float: left;width: 20%;height:34px;position: relative;}
.share ul li a{display: inline-block;width: 100%;height:100%;text-align: center;}
.icon2{position: absolute;left: 0;top:0;
    filter:alpha(opacity=0);
    -moz-opacity:0;
    -khtml-opacity: 0;
    opacity: 0;
}
.share ul li:hover .icon2{
    filter:alpha(opacity=100);
    -moz-opacity:1;
    -khtml-opacity: 1;
    opacity: 1;
    -webkit-transition: all 0.3s ease-in;
    -moz-transition: all 0.3s ease-in;
    -ms-transition: all 0.3s ease-in;
    -o-transition: all 0.3s ease-in;
    transition: all 0.3s ease-in;
}
/*标题字体之间的间距*/
.tit_l,.depar_tit span{letter-spacing:10px;}

/*关于我们*/
.matter{width: 100%;background-color: #fff;padding-top: 30px;padding-bottom: 45px;}
.matter_box{position:relative;}
.navL{width: 290px;position: absolute;top:0;left: 0;}
.navL_h{width: 100%;height: 60px;line-height: 60px;text-align:center;background-color: #b51c26;color:#fff;font-size: 22px;}
/*.navL_h span{display: inline-block;width: 28px;height: 26px;margin-bottom:-5px;background: url("../images/about/icon.png") no-repeat;}*/
.navL_h em{margin: 0 15px;}
.navL_h img{display: inline-block;margin-bottom: -5px;}
.ia{}
.ia2{display: none;}
.navUl{margin-bottom: 5px;background-color: #f5f5f5;}
.navUl li{width: 100%;padding: 0 2px;}
.navUl li a.navUl_a{display: block;height: 60px;line-height: 60px;font-size: 16px;color: #595856;border-top: dashed 1px #aaaaaa;}
.navUl li a.navUl_a em{float: left;margin-left: 55px;}
.navUl li a.navUl_a img{float: right;margin-top: 25px;margin-right: 30px;}
.navUl li.current{}
.navUl li.current a.navUl_a{color: #b51c26;border-left: solid 3px #ff9600;background-color: #f3eddd;border-bottom: dashed 1px #aaaaaa;}
